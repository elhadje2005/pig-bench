<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 
	<context-param>
		<param-name>debug</param-name>
		<param-value>false</param-value>
	</context-param>
	
	<servlet>
		<display-name>AddJob</display-name>
		<servlet-name>AddJob</servlet-name>
		<servlet-class>com.tinkerpop.bench.web.AddJob</servlet-class>
		<load-on-startup>1</load-on-startup>
	 </servlet>
	
	<servlet>
		<display-name>RunBenchmark</display-name>
		<servlet-name>RunBenchmark</servlet-name>
		<servlet-class>com.tinkerpop.bench.web.RunBenchmark</servlet-class>
		<load-on-startup>1</load-on-startup>
	 </servlet>
	
	<servlet>
		<display-name>ShowOutput</display-name>
		<servlet-name>ShowOutput</servlet-name>
		<servlet-class>com.tinkerpop.bench.web.ShowOutput</servlet-class>
		<load-on-startup>1</load-on-startup>
	 </servlet>

	<servlet-mapping>
		<servlet-name>AddJob</servlet-name>
		<url-pattern>/AddJob</url-pattern>
	</servlet-mapping>
 
	<servlet-mapping>
		<servlet-name>RunBenchmark</servlet-name>
		<url-pattern>/RunBenchmark</url-pattern>
	</servlet-mapping>
 
	<servlet-mapping>
		<servlet-name>ShowOutput</servlet-name>
		<url-pattern>/ShowOutput</url-pattern>
	</servlet-mapping>
 
</web-app>
